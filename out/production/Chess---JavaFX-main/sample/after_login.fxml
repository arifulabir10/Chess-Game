<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.after_loginController">
   <children>
      <ImageView fitHeight="444.0" fitWidth="825.0" layoutX="-133.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@back.png" />
         </image>
      </ImageView>
      <Button fx:id="logout" layoutX="308.0" layoutY="207.0" mnemonicParsing="false" onAction="#logoutbutton" prefHeight="57.0" prefWidth="156.0" style="-fx-background-color: CYAN;" text="Logout" textFill="#5c00ff">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Button>
      <Button fx:id="dashboard" layoutX="308.0" layoutY="299.0" mnemonicParsing="false" onAction="#dashboardbutton" prefHeight="64.0" prefWidth="156.0" style="-fx-background-color: GREEN;" text="Dashboard" textFill="WHITE">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Button>
      <Label fx:id="welcomeText" layoutX="193.0" layoutY="57.0" textFill="WHITE">
         <font>
            <Font size="44.0" />
         </font>
      </Label>
      <Label layoutX="219.0" layoutY="42.0" prefHeight="80.0" prefWidth="282.0" text="Welcome" textFill="WHITE">
         <font>
            <Font name="Sitka Small" size="54.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
